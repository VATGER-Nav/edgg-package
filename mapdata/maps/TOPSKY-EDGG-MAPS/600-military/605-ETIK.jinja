{%- set rnav = {
    "06": [
        'ANS',
        'IK410',
        'IK413',
        'IK700',
    ],
    "24": [
        'DKB',
        'IK402',
        'IK403',
        'IK405',
        'IK406'
    ]
} %}

MAP:ETIK RNAV 06
FOLDER:RNAV/RNP APP
ACTIVE:RWY:ARR:ETIK06:DEP:*
COLOR:standard
SYMBOL:VOR:ANS
{%- for fx in rnav['06'] if not fx in ['ANS'] %}
